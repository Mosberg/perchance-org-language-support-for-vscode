{
  "name": "perchance-org-language-support-for-vscode",
  "displayName": "Perchance.org Language Support for VSCode",
  "description": "Language Support for Perchance.org in Visual Studio Code.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mosberg/perchance-org-language-support-for-vscode"
  },
  "license": "MIT",
  "publisher": "Mosberg",
  "author": {
    "name": "Mosberg"
  },
  "icon": "./assets/img/favicon-1024x1024-white-bg.png",
  "main": "./src/extension.js",
  "activationEvents": [],
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "perchance",
        "aliases": [
          "Perchance",
          "perchance"
        ],
        "extensions": [
          ".per",
          ".perchance",
          ".prch"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "perchance",
        "scopeName": "source.perchance",
        "path": "./assets/syntaxes/perchance.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "perchance",
        "path": "./assets/snippets/perchance.json"
      }
    ]
  }
}
