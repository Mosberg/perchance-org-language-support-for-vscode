{
  "name": "perchance-org-language-support-for-vscode",
  "displayName": "Perchance.org Language Support for VSCode",
  "description": "Language Support for Perchance.org in Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "Mosberg",
  "author": {
    "name": "Mosberg"
  },
  "icon": "assets/img/favicon-1024x1024-white-bg.png",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "perchance",
        "aliases": [
          "Perchance",
          "perchance"
        ],
        "extensions": [
          ".per",
          ".perchance",
          ".prch"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "perchance",
        "scopeName": "source.perchance",
        "path": "./syntaxes/perchance.tmLanguage.json"
      }
    ]
  }
}
